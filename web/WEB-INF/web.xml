<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>CtrlNotasAlumno</servlet-name>
        <servlet-class>controlador_intranet_alumno_cursos.CtrlNotasAlumno</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CambiarImagenPerfil</servlet-name>
        <servlet-class>Controlador.CambiarImagenPerfil</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CtrlNotasAlumno</servlet-name>
        <url-pattern>/CtrlNotasAlumno.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CambiarImagenPerfil</servlet-name>
        <url-pattern>/CambiarImagenPerfil.do</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
